<div class="stepwizard">
  <div class="stepwizard-row setup-panel">
    <div class="stepwizard-step">
      <a href="#step-1" type="button" class="btn btn-info btn-circle-wizard">General Info</a>
    </div>
    <div class="stepwizard-step">
      <a href="#step-2" type="button" class="btn btn-default btn-circle-wizard" disabled="disabled">Creating Account</a>
    </div>
    <div class="stepwizard-step">
      <a href="#step-3" type="button" class="btn btn-default btn-circle-wizard" disabled="disabled">Creating Relation</a>
    </div>
  </div>
</div>
<br />
<%#= validation_errors_notifications(resource) %>
<%= devise_error_messages! %>

<div class="row setup-content" id="step-1">
  <%= render  "#{@shared}/general_info", :f => f %>
  <div class="row">
    <div class="col-lg-12 col-md-12">
      <div class="form-actions">
        <button class="btn btn-primary nextBtn btn-lg pull-right" type="button">Next</button>
        <%= link_to "Cancel", "javascript: void(0)", "data-url" => cancel_admissions_path, method: :post, class: "btn btn-default btn-lg cancel-admission", :onClick => "confirmation('Cancel Admission?', this);" %>
      </div>
    </div>
  </div>
</div>
<div class="row setup-content" id="step-2">
  <%= render "#{@shared}/student_account", :f => f %>
  <div class="row">
    <div class="col-lg-12 col-md-12">
      <div class="form-actions">
        <button class="btn btn-primary nextBtn btn-lg pull-right" type="button">Next</button>
        <%= link_to "Cancel", "javascript: void(0)", "data-url" => cancel_admissions_path, method: :post, class: "btn btn-default btn-lg cancel-admission", :onClick => "confirmation('Cancel Admission?', this);" %>
      </div>
    </div>
  </div>
</div>
<div class="row setup-content" id="step-3">
  <%= render "#{@shared}/guardian_relation", :f => f %>
  <div class="row">
    <div class="col-lg-12 col-md-12">
      <div class="form-actions">
        <%= f.button :button, 'Enroll Now', :class => 'btn btn-success nextBtn btn-lg pull-right', data: { disable_with: "<i class='fa fa-spinner fa-spin'></i> Registering"} %>
        <%= link_to "Cancel", "javascript: void(0)", "data-url" => cancel_admissions_path, method: :post, class: "btn btn-default btn-lg cancel-admission", :onClick => "confirmation('Cancel Admission?', this);" %>	
      </div>
    </div>
  </div>
</div>
