<strong><%= semester.name %></strong>
<% if semester.courses.present? %>
	<table class="table table-striped table-hover table-checked-list-box">
		<thead>
			<tr>
				<th>Name</th>
				<th>Code</th>
				<th></th>
			</tr>
		</thead>
		<tbody>
			<% semester.courses.each do |course| %>
				<tr>
					<td><%= course.name %></td>
					<td><%= course.code %></td>
					<td></td>
				</tr>
			<% end %>
		</tbody>
	</table>						
<% else %>
	<blockquote><p>No courses assigned</p></blockquote>
<% end %>

