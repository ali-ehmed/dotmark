<!DOCTYPE html>
<html>
  <head>
    <%= render "layouts/shared/head", title: "Dotmark - #{head_title}" %>
  </head>
  <body>
    <header>
      <%= render 'layouts/shared/navigation' %>
    </header>
    <main role="main">
      <div class="container">
        <%= render 'layouts/shared/breadcrumbs' %>
        <%= render 'layouts/shared/messages' %>
        <%= render 'layouts/shared/institute_settings' %>
        <%= render 'layouts/shared/settings' %>
        <%= yield %>
      </div>
    </main>
    <footer>
      <%= render 'layouts/shared/footer' %>
    </footer>
  </body>
</html>
